{
  "canisters": {
    "dao_backend": {
      "main": "src/dao_backend/main.mo",
      "type": "motoko"
    },
    "governance": {
      "main": "src/dao_backend/governance/main.mo",
      "type": "motoko",
      "dependencies": [
        "dao_backend",
        "staking"
      ]
    },
    "staking": {
      "main": "src/dao_backend/staking/main.mo",
      "type": "motoko"
    },
    "treasury": {
      "main": "src/dao_backend/treasury/main.mo",
      "type": "motoko"
    },
    "proposals": {
      "main": "src/dao_backend/proposals/main.mo",
      "type": "motoko"
    },
    "assets": {
      "main": "src/dao_backend/assets/main.mo",
      "type": "motoko",
      "init": { "args": "(null, false)" }
    },
    "dao_frontend": {
      "dependencies": [
        "dao_backend",
        "governance",
        "staking",
        "treasury",
        "proposals",
        "assets"
      ],
      "source": [
        "src/dao_frontend/dist"
      ],
      "type": "assets",
      "workspace": "dao_frontend",
      "declarations": {
        "output": "src/dao_frontend/src/declarations"
      }
    },
    "internet_identity": {
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "frontend": {},
      "type": "custom",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "playground": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}